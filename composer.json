{
  "require": {
    "craftcms/cms": "3.0.34",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "2.1.6",
    "craftcms/aws-s3": "^1.0",
    "bolden/htmlcache": "1.0.4",
    "craftcms/element-api": "2.5.4",
    "verbb/super-table": "2.0.14",
    "verbb/field-manager": "2.0.4",
    "ether/simplemap": "3.3.4",
    "abryrath/craft-sync-db": "dev-master"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
        "abryrath\\databasehelper\\": "modules/databasehelper",
        "abryrath\\wedding\\": "modules/app/src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
