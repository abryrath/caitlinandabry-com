{% extends '_layouts/default' %}
{% block content %}
    {# {% cache globally unless getenv('APP_ENV') == 'development' %} #}
        {% set hero = entry.pageHeroImage %}
        {% if hero | length %}
            <div class="Hero">
                <img src="{{hero.one.getUrl}}"/>
            </div>
        {% endif %}

        {% for content in entry.ourStoryContent %}
            {% include '_partials/' ~ content.type.handle with {
                content: content
            } only %}
        {% endfor %}
    {# {% endcache %} #}
{% endblock %}